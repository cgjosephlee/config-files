{
  "title": "Custom 3: Tmux",
  "rules": [
    {
      "description": "Tmux Prefix Mode [caps_lock as trigger key] (modified)",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "caps_lock",
            "modifiers": {
              "optional": [
                "shift"
              ]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "tmux_prefix_mode",
                "value": 1
              }
            }
          ],
          "to_if_alone": [
            {
              "key_code": "caps_lock"
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "caps_lock",
              "hold_down_milliseconds": 100
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "tmux_prefix_mode",
                "value": 0
              }
            }
          ],
          "parameters": {
            "basic.to_if_alone_timeout_milliseconds": 250,
            "basic.to_if_held_down_threshold_milliseconds": 250
          },
          "conditions": [
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^com\\.apple\\.Terminal$",
                "^com\\.googlecode\\.iterm2$",
                "^co\\.zeit\\.hyperterm$",
                "^co\\.zeit\\.hyper$",
                "^io\\.alacritty$",
                "^net\\.kovidgoyal\\.kitty$"
              ]
            }
          ]
        }
      ]
    },
